<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default">
        <action name="list" class="example.Welcome">
            <result>Mainpage.jsp</result>
        </action>
        <action name="registered" class="example.Login" >
            <result>registered.html</result>
        </action>
        <action name="forgetpwd" class="example.Login" >
            <result>forgetpwd.html</result>
        </action>
        
        <!--
        <action name="list_*" class="my.First" method="{1}">
            <result type="redirect">list</result>
        </action>
        -->
    </package>
</struts>